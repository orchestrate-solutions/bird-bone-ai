# Bird Bone AI - Environment Configuration Template
# Copy this file to .env and fill in your specific values

# Application Environment
ENVIRONMENT=development

# Python Configuration
PYTHONPATH=/app

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# DVC Configuration
DVC_CACHE_DIR=.dvc/cache
DVC_REMOTE_URL=

# Git LFS Configuration
GIT_LFS_ENABLED=true

# Model Configuration
MODEL_DIR=./models
DATA_DIR=./data

# API Configuration (if applicable)
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=false

# Database Configuration (if applicable)
# DATABASE_URL=postgresql://username:password@localhost:5432/bird_bone_ai

# External Services (if applicable)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_DEFAULT_REGION=us-east-1

# Monitoring and Analytics (if applicable)
# SENTRY_DSN=
# ANALYTICS_ENABLED=false

# Security
# SECRET_KEY=your-secret-key-here
# JWT_SECRET=your-jwt-secret-here

# Performance
# MAX_WORKERS=4
# CACHE_TTL=3600
